<odoo>
    <data>
        <record model="ir.ui.view" id="kki_lift_history_form">
            <field name="name">kki_lift_history form</field>
            <field name="model">kki_lift.history</field>
            <field name="arch" type="xml">
                <form>
                    <sheet>
                        <group>
                            <group>
                                <field name="name"/>
                                <field name="fork_1"/>
                                <field name="back_1"/>
                                <field name="chain_1"/>
                                <field name="mast_1"/>
                                <field name="tire_1"/>
                                <field name="handle_1"/>
                                <field name="handle_2"/>
                                <field name="break_1"/>
                            </group>
                            <group>
                                <field name="check_date"/>
                                <field name="horn_1"/>
                                <field name="volt_meter_1"/>
                                <field name="cooling_water"/>
                                <field name="oil_1"/>
                                <field name="battery"/>
                            </group>
                            <group>
                                <field name="image" widget="image"/>
                            </group>
                        </group>
                    </sheet>
                </form>
            </field>
        </record>
    </data>
</odoo>